function goToPage(t)
{
	var type = t;
	var tr, ex;
	var index;
	
	switch (type)
	{
		case 1 :
			index = document.form1.s2.selectedIndex;

			tr   = document.form1.s2.options[index].value;
			if (!tr) 
			{
				 alert("you must select a group of traits and then select a trait.");
				 return;
			}
			
			ex = get_ex_by_tr(tr, 2);
			  			
			location = "distribution.htm?page_type=1&tr="+tr+"&ex=" + ex;
			
			
			break;
			
		case 2 :

			index = document.form1.s3.selectedIndex;

			ex   = document.form1.s3.options[index].value;
			
			if (!ex)
			{
			   alert("you must specify experiment");
				 return;
			}
			

			if (ex.search("Global") != -1)
				 location = "trait_correlation.htm?page_type=3&tr=undef&ex=" + ex;
			else
				 location = "trait_correlation.htm?page_type=3&tr=undef&ex=" + ex;
		 
			
			break;
		case 3:
			index = document.form1.s5.selectedIndex;

			tr   = document.form1.s5.options[index].value;
		
			location = "experiment_correlation.htm?page_type=1&tr=" + tr + "&ex=undef";
		
			break;
		case 4:
			location = "http://zamir.sgn.cornell.edu/cgi-bin/trait_search/traits.pl";
			break;
			
			
		case 5:
			var ind = document.form1.t1.selectedIndex;
			var tr1 = document.form1.t1.options[ind].value;;
			ind     = document.form1.t2.selectedIndex;
			var tr2 = document.form1.t2.options[ind].value;
			
			if (!tr1  ||  !tr2)
			{
			 	 alert ("You must select 2 traits");
				 return;
			}
			
			if (tr1.search(tr2) != -1   &&  tr2.search(tr1) != -1)
			{
			 	 alert ("You must select 2 different traits !");
				 return;
			}
			
			location = "global_correlation.htm?page_type=1&tr1=" + tr1 +"&tr2="+tr2+"&front=1";
			break;	
			
	}
	


}



function set_radio(t)
{
	var type = t;
			
	if (type == 0)
	{
		document.form1.b.checked = false;
	}
	if (type == 1)
	{
		document.form1.a.checked = false;
	}
	
	
}


