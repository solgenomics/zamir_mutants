function goToPage_1()
{
	var index = document.form1.s1.selectedIndex;
	var temp_tr = document.form1.s1.options[index].value;
	
	if (!temp_tr)
	{
	   alert("you must select experiment");
		 return;
	}
	
	var win = window.open("distribution.htm?page_type=2&tr=" + tr + "&ex=" + temp_tr);
	
}





function goToPage(t)
{
	typ = t;
	if (typ != 1)
	{
		if (document.form1.r1  &&  document.form1.r1.checked == true)
		{			
			location = "experiment_correlation.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;
		}		
		if (document.form1.r2.checked == true)
		{
			location = "tail_chart.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;
		}
		if (document.form1.r3.checked == true)
		{
			location =  "qtl_map.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;
		}
		
	}
	else
	{
		location = "home.htm";
	}
}


function set_radio(t)
{
	var type = t;
			
	if (type == 1)
	{
		document.form1.r2.checked = false;
		document.form1.r3.checked = false;
	}
	if (type == 2)
	{
		document.form1.r1.checked = false;
		document.form1.r3.checked = false;
	}
	if (type == 3)
	{
		document.form1.r1.checked = false;
		document.form1.r2.checked = false;
	}
}

function set_radio_1(t)
{
	var type = t;
			
	if (type == 2)
	{
		document.form1.r3.checked = false;		
	}
	if (type == 3)
	{
		document.form1.r2.checked = false;		
	}
	
}


