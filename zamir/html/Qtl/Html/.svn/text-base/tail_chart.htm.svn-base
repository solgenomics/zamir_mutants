function goToPage_1()
{
	var index = document.form1.s2.selectedIndex;
	var tempo_ex = document.form1.s2.options[index].value;
	
	if (!tempo_ex)
	{
	   alert("you must select experiment");
	   return;
	}
	
	var win = window.open("tail_chart.htm?page_type=2&tr=" + tr + "&ex=" + tempo_ex);
	
}


function goToPage(t)
{
	var type = t;
	var index;
	var arr;
	
	if (type == 0)
	{
		location =  "experiment_correlation.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;

	}
	if (type == 1)
	{
		location =  "qtl_map.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;

	}
	if (type == 2)
	{
		location =  "distribution.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;
	}
	if (type == 3)
	{
		location =  "home.htm";
	}
	if (type == 4)
	{
		location =  "tail_chart_1.htm?page_type=" + page_type + "&tr=" + tr + "&ex=" + ex;
	}
	if (type == 5)
	{
	 	index = document.form1.s1.selectedIndex;
		il       = document.form1.s1.options[index].value;
		if (!il)
		{
		   alert("you must select IL");
		   return;
		}
		
		arr     = /IL(\d+)/g.exec(il);
				
		location = "il_analysis.htm?page_type=" + page_type + "&tr="+tr+"&ex="+ex+"&ch="+arr[1]+"&il="+il;
		
	
	}
	if (type == 6)
	{
		var PageIndex=document.form1.s1.selectedIndex;
		var il = document.form1.s1.options[PageIndex].value;
	
		if (!il)
		{
			  alert("You must select IL");
			  return;
		}	
	
		window.open("http://zamir.sgn.cornell.edu/cgi-bin/IL_cards/qtl.cgi?menu=il_card&il="+il);
		return;
	
	}

}


function set_radio(t)
{
	var type = t;
			
	if (type == 1)
	{
		document.form1.r2.checked = false;
	}
	if (type == 2)
	{
		document.form1.r1.checked = false;
	}
	
}

//-->

</script>

