


USE naama;

###########################   il_cards table   ##################################


CREATE TABLE il_cards (
   id           INT UNSIGNED NOT NULL,		# Unique identifier
   genotype     VARCHAR(10),
   number       INT UNSIGNED,	
   accession    VARCHAR(10),				
   chromosome   INT UNSIGNED NOT NULL,
   north        VARCHAR(10),
   south        VARCHAR(10),
   genes        VARCHAR(100),
   observation  VARCHAR(100),
   fine_mapping VARCHAR(10),
   publication  VARCHAR(10),
   allozymes    VARCHAR(30)
         
);

